    "@testing-library/user-event": "^14.6.1",
    "axios": ">=1.8.2",
    "carbon-components": "^11.0.0",
    "carbon-components-react": "^8.67.1",
    "carbon-icons": "7.0.7",
    "d3": "^7.0.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.23.24",
    "http-proxy-middleware": "^3.0.0",
    "i18next": "^23.0.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "javascript-time-ago": "^2.5.12",
    "mapbox-gl": "^2.15.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.38",
    "prettier": "^3.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^13.0.0",
    "react-moment": "^1.1.3",
