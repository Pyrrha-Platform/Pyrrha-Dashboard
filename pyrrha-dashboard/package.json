{
  "name": "pyrrha-dashboard",
  "version": "0.2.0",
  "license": "Apache-2.0",
  "dependencies": {
    "@carbon/react": "^1.67.0",
    "@carbon/charts": "^1.27.0",
    "@carbon/charts-react": "^1.27.0",
    "@carbon/grid": "^11.30.0",
    "@carbon/icons-react": "^11.54.0",
    "@carbon/layout": "^11.30.0",
    "@carbon/themes": "^11.30.0",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-controls": "^8.4.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.8.2",
    "d3": "^7.9.0",
    "formik": "^2.4.6",
    "framer-motion": "^11.15.0",
    "http-proxy-middleware": "^3.0.3",
    "i18next": "^23.0.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "javascript-time-ago": "^2.5.12",
    "mapbox-gl": "^2.15.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.38",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.0",
    "react-moment": "^1.1.3",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "sass": "^1.81.0"
  },
  "scripts": {
    "start-ui": "DISABLE_ESLINT_PLUGIN=true react-scripts build && npx serve -s build -p 3000",
    "start-main-api": "cd api-main && FLASK_APP=api.py ./venv/bin/flask run --no-debugger --port 5000",
    "start-auth-api": "cd api-auth && node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "format:prettier": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\"",
    "format:black": "cd api-main && ./venv/bin/black .",
    "format:staged": "prettier --write",
    "lint": "yarn eslint .",
    "lint:fix": "yarn eslint . --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-testing-library": "^7.13.3",
    "globals": "^16.4.0"
  }
}
